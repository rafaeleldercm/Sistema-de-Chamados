   <div role="main" class="container_12" id="content-wrapper">
      <div id="main_content">
         <div style='margin: 15px '>
            <div style='float: left'>
            </div>
            <div style='float: right'>
               <%= will_paginate_custom @chamados if @chamados %>
            </div>
        </div>

        <div class="grid_12">
           <div class="box">
              <div class="header">
                  <%= image_tag( "#{$ICO1_PATH}/application-detail.png") %>
                  <h3>Chamados</h3><span></span>
              </div>
              <div class="content" style='padding: 0'>
                 <table id="table-example" class="table" >
                    <thead>
                       <tr>
                          <th>ID</th>
                          <th>Status</th>
                          <th>Solicitante</th>
                          <th>Sistema</th>
                          <th>Tipo de Chamado</th>
                          <th>Atendente</th>
                          <th>Criado em </th>
                          <th colspan='2'> Ações  </th>
                       </tr>
                    </thead>
                 <tbody>
                    <% @chamados.each do |chamado| %>
                       <tr>
                          <td><%=h chamado.id                 %></td>
                          <td><%=h chamado.status_desc        %></td>
                          <td><%=h chamado.usuario.name       %></td>
                          <td><%=h chamado.sistema.nome       %></td>
                          <td><%=h chamado.tipo_chamado.nome  %></td>
                          <td><%=h chamado.atendente.name     if chamado.atendente %></td>
                          <td><%=h chamado.created_at.to_s_br if chamado.created_at %></td>
                          <td><%= link_to image_tag("#{$ICO1_PATH}/application-search-result.png", :title => 'Visualizar'), chamado %></td>
                       </tr>
                    <% end %>
                 </tbody>
                 </table>
              </div> <!-- End of .content -->
              <div class="clear"></div>
           </div> <!-- End of .box -->
        </div> <!-- End of .grid_12 -->
        <br/>
     </div> <!-- End of #main_content -->
     <div class="clean"></div>
   </div> <!-- End of #content-wrapper -->

